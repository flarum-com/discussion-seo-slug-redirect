{
    "name": "flarum/discussion-seo-slug-redirect",
    "description": "Redirects from raw path to discussions",
    "keywords": [
        "extension",
        "flarum"
    ],
    "license": "mit",
    "type": "flarum-extension",
    "authors": [
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "https://luceos.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "flarum/core": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\DiscussionSeoSlugRedirect\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Flarum\\DiscussionSeoSlugRedirect\\Tests\\": "tests/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Discussion Seo Slug Redirect",
            "category": "feature",
            "icon": {
                "image": "resources/assets/logo.png",
                "backgroundColor": "#E3E7F1",
                "backgroundSize": "60%",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center"
            }
        },
        "extiverse": {
            "discuss": "https://discuss.flarum.org/d/?"
        }
    }
}
